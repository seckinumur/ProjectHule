@model List<Entity.Model.Urun>
@{
    ViewBag.Title = "ExcellTest";
}
<div id="page-wrapper">
        <div class="row">

            <div class="col-lg-12">
                <h1 class="page-header">Excel Database Yükleme İşlemi Test Sayfası</h1>
            </div>
        </div>
        <div class="col-lg-12">
            <form method="post" enctype="multipart/form-data">
                <div class="form-group col-md-3">
                    <label class="text-primary">Excel Dosyası Seçin</label>
                    <input class="btn btn-default" type="file" name="UploadedFile" />
                </div>
                <div class="form-group col-md-4">
                    <p class="text-info">Uyumlu Bir Excel Dosyayı Sisteme Yükler. Mevcut İşlemin Bitmesini Bekleyin.</p>
                    <input type="submit" class="btn btn-danger btn-block" name="Submit" value="Yükle" />
                </div>
                <div class="form-group col-md-5">
                    <p class="text-primary">Dikkat! Bağlantı Hızına Göre Bu İşlem 63 bine yakın Kayıt Nedeniyle 15 dk Sürmektedir.</p>
                </div>
            </form>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Tüm Ürün Database Gösteriliyor.
                    </div>

                    <div class="panel-body">
                        <h3 class="h3">Burada Bir Tablo olacak. DataTable 1.4</h3>

                    </div>
                </div>
            </div>
        </div>

    <div class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title">Lütfen Bekleyiniz...</h4>
                </div>
                <div class="modal-body">
                    <p>Excel Yükleme İşlemi Boyunca İnternet bağlantınızı kesmeyin ve tarayıcı kapatmayın!</p>
                    <hr />
                    <p>Bu İşlem Yaklaşık Olarak 15dk sürebilir.</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>

</div>
<script type="text/javascript">
    
</script>